{
    "functions": {
      "api/*.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }